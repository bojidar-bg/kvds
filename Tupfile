
import CC=gcc
CCFLAGS += -g
CCFLAGS += -O3 -fno-omit-frame-pointer
CCFLAGS += -DNDEBUG
import LD=gcc

: foreach src/algo/*.c |> $(CC) %f $(CCFLAGS) -c -o %o |> obj/algo/%B.o {objs}
: foreach src/*.c |> $(CC) %f $(CCFLAGS) -c -o %o |> obj/%B.o {objs}
: {objs} |> $(LD) %f -o %o |> bin/kvds

.gitignore
